<!--  Pick seeds by Season-->
<div id="season-section" class="py-20 sm:py-26">
  <div class="mx-auto max-w-7xl px-6 lg:px-8">
    <div class="mx-auto max-w-2xl lg:mx-0">
      <h2
        style="font-family: Montagu Slab; font-size: 25px"
        class="font-bold text-gray-900 sm:text-4xl py-8"
      >
        Pick seeds by Season
      </h2>
    </div>
    <ul
      role="list"
      class="mx-auto mt-6 grid max-w-2xl grid-cols-1 gap-x-8 gap-y-16 sm:grid-cols-2 lg:mx-0 lg:max-w-none lg:grid-cols-4"
    >
      <li>
        <a href="/summer">
          <img
            class="aspect-[4/3] w-full rounded-2xl hover:shadow-lg hover:border-1 hover:border-green-800 hover:bg-green-300"
            src="./images/summer.png"
            alt="summer"
          />
        </a>
      </li>
      <li>
        <a href="/autumn">
          <img
            class="aspect-[4/3] w-full rounded-2xl hover:shadow-lg hover:border-1 hover:border-green-800 hover:bg-green-300"
            src="./images/autumn.png"
            alt="autumn"
          />
        </a>
      </li>
      <li>
        <a href="/winter">
          <img
            class="aspect-[4/3] w-full rounded-2xl hover:shadow-lg hover:border-1 hover:border-green-800 hover:bg-green-300"
            src="./images/winter.png"
            alt="winter"
          />
        </a>
      </li>
      <li>
        <a href="/spring">
          <img
            class="aspect-[4/3] w-full rounded-2xl transition-transform transform hover:shadow-lg hover:border-1 hover:border-green-800 hover:bg-green-300"
            src="./images/spring.png"
            alt="spring"
          />
        </a>
      </li>
    </ul>
  </div>
</div>

{{! spacing }}
<div class="py-20"></div>

