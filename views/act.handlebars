<header class="font-bold text-xl my-4">Spring</header>
<!-- Swiper Container -->
<div class="mx-auto max-w-screen-md overflow-hidden">
  <div class="swiper mySwiper">
      <div class="swiper-wrapper">
          {{#each statePlants as |statePlant|}}
          {{#if statePlant.spring}}
          <div class="swiper-slide">
              <div class="card {{statePlant.plantType}} bg-white shadow-lg rounded-lg" style="width: 100px;">
                  <div class="card-body p-4">
                      <h5 class="card-title font-semibold">{{statePlant.plantName}}</h5>
                      <h6 class="card-subtitle mb-2 text-gray-500">{{statePlant.plantSeason}}</h6>
                      <p class="card-text text-sm">{{statePlant.plantDescription}}</p>
                      <p class="card-text text-sm">{{statePlant.plantLocation}}</p>
                  </div>
              </div>
          </div>
          {{/if}}
          {{/each}}
      </div>
      <!-- Add Pagination -->
      <div class="swiper-pagination"></div>
      <!-- Add Navigation -->
      <div class="swiper-button-next"></div>
      <div class="swiper-button-prev"></div>
  </div>
</div>



{{!--the html data-theme="cupcake" is for daisyui setup --}}
<html lang="en" data-theme="cupcake">
  <div class="py-24 sm:py-32">
    <div class="flex justify-center ml-20 mt-20">
      <div>
        <h2
          style="font-family: Montagu Slab; font-size: 30px;"
          class="font-bold mb-5  text-center text-gray-900 sm:text-4xl"
        >
          Spring
        </h2>
        <ul
          role="list"
          class="mx-auto mt-6 grid max-w-2xl grid-cols-1 gap-x-8 gap-y-16 sm:grid-cols-2 lg:mx-0 lg:max-w-none lg:grid-cols-4"
        >
          {{#each statePlants as |statePlant|}}
           {{#if statePlant.spring}}
            <li>
              <div class="card bg-base-100 shadow-xl image-full">
                <figure>
                  <img src="../images/card-bg.jpg" alt="card-bg" />
                </figure>
                <div class="card-body">
                  <h2 class="card-title">{{plantName}}  </h2>
                  <p class="bg-neutral-content text-neutral p-2">
                    {{plantDescription}}
                  </p>
                  <div class="card-actions justify-end">
                    <button style="background-color: #f6e2bd6e;" class="btn">
                       {{plantEmoji plantType}}    {{plantSeason}}
                    </button>
                  </div>
                </div>
